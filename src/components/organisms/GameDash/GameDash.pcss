.game-dash {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%
}

.game-dash__wrapper {
  width: 300px;
  height: 300px;
}

.game_dash__letter-container {
  transition: transform 1s;
  transform-style: preserve-3d;
  cursor: pointer;
  position: relative;
  display:flex;
}
