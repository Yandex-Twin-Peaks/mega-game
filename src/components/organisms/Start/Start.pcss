.start-container {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%; /* Full width */
  height: 100%; /* Full height */
  padding-top: 60px;
}

.start-container__title {
  font-size: 25px;
  margin-bottom: 30px;
}

.start-container__button {
  font-size: 20px;
  font-family: Bad Script;
  padding: 25px;
  cursor: pointer;
  background-color: white;
}
