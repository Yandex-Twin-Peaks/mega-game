{
  "name": "yandex-practicum-2-mega-game",
  "version": "0.0.1",
  "description": "Yandex Practicum Middle Frontend. Twin Peaks Team. Mega Game Project",
  "scripts": {
    "dev:watch:client": "webpack --config webpack/ssr-dev.config.ts --watch",
    "dev:build:client": "webpack --config webpack/dev.config.ts",
    "dev:build:server": "tsc --project ./src/server -p ./src/server/tsconfig.dev.json && npm run dev:build:assets",
    "dev:build:assets": "mkdir ./dist/src/assets && cp -R ./static/* ./dist/src/assets/ && cp ./src/index.html ./dist/src/",
    "dev:start:server": "MODE=development node ./dist/src/server/index.js",
    "dev:clean": "rm -rf dist",
    "build:client": "webpack --config webpack/prod.config.ts",
    "build:ssr": "webpack --config webpack/ssr.config.ts",
    "build:server": "tsc --project ./src/server -p ./src/server/tsconfig.prod.json && npm run build:assets",
    "build:assets": "mkdir ./build/src/assets && cp -R ./static/* ./build/src/assets/ && cp ./src/index.html ./build/src/",
    "start:server": "node ./build/src/server/index.js",
    "build:clean": "rm -rf build",
    "clean": "npm run dev:clean && npm run build:clean",
    "dev": "npm run dev:clean && npm run dev:build:server && npm run dev:build:client && npm run dev:start:server & npm run dev:watch:client",
    "start": "npm run build:clean && npm run build:server && npm run build:client && npm run build:ssr && npm run start:server"
  },
  "browser": {
    "[module-name]": false
  },
  "repository": {
    "url": "git@github.com:Yandex-Twin-Peaks/mega-game.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "author": "Twin Peaks Team",
  "license": "GPL-3.0",
